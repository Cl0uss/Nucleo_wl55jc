{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Zephyr Build (auto select project)",
      "type": "shell",
      "command": "bash",
      "args": [
        "-c",
        "./build_project.sh"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": "$gcc",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
